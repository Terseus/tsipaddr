{
    "name": "tsipaddr",
    "version": "0.1.0",
    "scripts": {
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "test": "npm run build && mocha dist/tests/*.js",
        "build": "npm run clean && tsc --outDir dist/",
        "pack": "npm run build && npm pack",
        "clean": "rm -rf dist/",
        "cleandoc": "rm -rf docs/",
        "doc": "npm run cleandoc && typedoc src/ --readme none"
    },
    "main": "./dist/src/index.js",
    "types": "./dist/src/index.d.ts",
    "engines": {
        "node": "^8.7.0"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/mocha": "^2.2.43",
        "@types/node": "^8.0.47",
        "babel-cli": "^6.24.1",
        "babel-preset-env": "^1.5.1",
        "mocha": "^3.4.2",
        "tslint": "^5.4.3",
        "typedoc": "^0.9.0",
        "typescript": "^2.3.4"
    }
}
